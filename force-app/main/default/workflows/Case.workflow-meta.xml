    <rules>
        <fullName>Escalated Case</fullName>
        <actions>
            <name>Escalated</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1</booleanFilter>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Escalated</value>
        </criteriaItems>
        <description>Auto populate Escalated field checkbox when case status is changed to escalated.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Release Milestone Time</fullName>
        <actions>
            <name>Reupdate_Stopped</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Uncheck the stopped field after case status is changed from &apos;on hold&apos;.</description>
        <formula>AND (    ISCHANGED( Status ),   TEXT( PRIORVALUE( Status ))=&quot;On Hold&quot; )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Stope Milestone Time</fullName>
        <actions>
            <name>Update_Stopped</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>On Hold</value>
        </criteriaItems>
        <description>Trigger the field update that check the stopped field.</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
